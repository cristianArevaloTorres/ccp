@model IEnumerable<template01.Models.consultacliente>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";



}

<link href="~/assets/pages/css/about.min.css" rel="stylesheet" type="text/css" />



<!-- BEGIN PAGE BAR -->
<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <a href="index">Home</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span>Asesoria Personal</span>
        </li>
    </ul>
    <div class="page-toolbar">
        <img src="~/assets/pages/img/banners/office-336368_1920.jpg" />
    </div>
</div>
<!-- END PAGE BAR -->
<!-- BEGIN PAGE TITLE-->

<h1 class="page-title">
    Consulta de clientes
    <small></small>
</h1>
<!-- END PAGE TITLE-->
<!-- END PAGE HEADER-->
<!-- BEGIN CONTENT HEADER -->

<input type="hidden" id="proceso" value="0">



<div class="row margin-bottom-25 about-author">
    <div class="col-lg-12 col-xs-12 col-sm-12">
        <div class="col-lg-12 col-xs-12 col-sm-12">
            <div class="portlet light ">
                <div class="portlet-title tabbable-line">
                    <div class="caption">
                        <i class="icon-bubbles font-dark hide"></i>
                        <span class="caption-subject font-dark bold uppercase">Tablero de Clientes</span>
                    </div>
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#portlet_comments_2" data-toggle="tab" aria-expanded="true">Clientes Nutricion</a>
                        </li>
                    </ul>
                </div>
                <div class="portlet-body">
                    <div class="tab-content">
                        <div class="tab-pane active" id="portlet_comments_2">
                            <!-- BEGIN: Comments -->
                            <table class="table table-striped table-bordered table-hover" id="tab_alimentos">
                                <thead>
                                    <tr role="row">
                                        <th>#folio</th>
                                        <th>Nombre </th>
                                        <th>Apellido Paterno </th>
                                        <th>Apellido Materno </th>
                                        <th>Edad </th>
                                        <th>Inicio de membresia</th>
                                        <th>Termino de membresia</th>
                                        <th>Vigente</th>


                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        string VIGENCIAINI = "";
                                        if (Model != null)
                                        {


                                            foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>@item.idusuario</td>
                                                    <td>@item.nombre</td>
                                                    <td>@item.apellidoP</td>
                                                    <td>@item.apellidoM</td>
                                                    <td>@item.edad</td>
                                                    <td>
                                                        @{
                                                            if (item.registro == "SR")
                                                            {
                                                                VIGENCIAINI = "--";
                                                            }
                                                            else
                                                            {
                                                                VIGENCIAINI = item.vigenciaini;
                                                            }
                                                        }
                                                        @VIGENCIAINI
                                                    </td>
                                                    <td>
                                                        @{
                                                            if (item.registro == "SR")
                                                            {
                                                                VIGENCIAINI = "--";
                                                            }
                                                            else
                                                            {
                                                                VIGENCIAINI = item.vigenciafin;
                                                            }
                                                        }
                                                        @VIGENCIAINI
                                                    </td>

                                                    <td>
                                                        @{
                                                            if (item.registro == "SR")
                                                            {
                                                                VIGENCIAINI = "Aun no se tiene un pago registrado";
                                                            }
                                                            else if (item.diasrest > 0)
                                                            {
                                                                VIGENCIAINI = "Vigente";
                                                            }
                                                            else if (item.diasrest < 0)
                                                            {
                                                                VIGENCIAINI = "Vencido";
                                                            }
                                                        }
                                                        @VIGENCIAINI
                                                    </td>
                                                    <td>  </td>
                                                </tr>
                                            }
                                        }

                                    }
                                </tbody>
                            </table>

                            <!-- END: Comments -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



</div>


<script>

</script>


